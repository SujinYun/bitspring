<mapper namespace="Cal">

<!-- 모든 리스트 -->

<select id="getCal" parameterType="bit.com.a.model.BbsParam" resultType="bit.com.a.model.BbsDto">
	SELECT SEQ, ID, TITLE, CONTENT, RDATE, WDATE
	FROM CALENDAR
	WHERE ID=#{id}
</select>
		


